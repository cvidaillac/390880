        "",
        "var listTasks = $data.listValidationTasks;",
        "",
        "for(var i=0; i < listTasks.length; i++)",
        "{",
        "    if(!listEntites.includes(listTasks[i].entiteName))",
        "    {",
        "        listEntites.push(listTasks[i].entiteName);",
